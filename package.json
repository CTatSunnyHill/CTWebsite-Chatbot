{
  "name": "chatbot",
  "version": "1.0.0",
  "description": "ChatBot for Sunny Hill Health Centre",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "backend": "nodemon index.js",
    "frontend": "npm run start --prefix client",
