    "actions-on-google": "^2.14.0",
    "chalk": "^2.4.1",
    "dialogflow": "^1.2.0",
    "dialogflow-fulfillment": "^0.6.1",
    "express": "^4.16.4",
    "mongodb": "^3.1.10",
    "mongoose": "^8.2.2",
    "structjson": "^1.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "http-proxy-middleware": "^2.0.6",
    "nodemon": "^1.18.5"
  }
